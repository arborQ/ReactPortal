{
  "name": "react-portal-business",
  "version": "0.0.1",
  "private": true,
  "description": "A frontend business portal with React, Redux and Typescript",
  "scripts": {
    "start": "node ./bin/dev.server.js",
    "mock": "node ./bin/mock.server.js",
    "build": "webpack-cli -p --progress --colors && sw-precache --root=public --config=./sw-precache-config.js --verbose",
    "sw": "sw-precache --root=public --config=./sw-precache-config.js --verbose",
    "serve": "concurrently --kill-others \"webpack-cli --watch\" \"firebase serve -p 8080\"",
    "deploy": "webpack-cli -p --progress --colors &&  sw-precache --root=public --config=./sw-precache-config.js --verbose && firebase deploy",
    "test": "cls && jest --notify --silent --config=testconfig.json",
    "test-debug": "cls && jest --notify --config=testconfig.json",
    "cypress": "cypress open",
    "lint:totals": "tslint -c tslint.json --project tsconfig.json -s node_modules/tslint-totals-formatter/formatters -t totals",
    "lint:run": "tslint -c tslint.json --project tsconfig.json --format stylish",
    "lint:fix": "tslint -c tslint.json --project tsconfig.json --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/enzyme": "2.8.9",
    "@types/express": "^4.16.0",
    "@types/firebase": "^3.2.1",
    "@types/history": "4.5.0",
    "@types/jest": "21.1.1",
    "@types/node": "7.0.13",
    "@types/react": "16.0.7",
    "@types/react-dom": "15.5.5",
    "@types/react-hot-loader": "3.0.3",
    "@types/react-redux": "5.0.9",
    "@types/react-router": "4.0.15",
    "@types/react-router-dom": "4.0.8",
    "@types/react-test-renderer": "15.5.4",
    "@types/redux-actions": "1.2.4",
    "@types/socket.io-client": "1.4.32",
    "@types/systemjs": "0.20.2",
    "@types/webpack": "2.2.15",
    "@types/webpack-env": "1.13.0",
    "awesome-typescript-loader": "^5.0.0-1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "cypress": "1.0.2",
    "dialog-polyfill": "^0.4.9",
    "enzyme": "^3.3.0",
